<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ceiec.twmp.tmp.mapper.TwmpLogLoginMapper" >
  <resultMap id="BaseResultMap" type="com.ceiec.twmp.tmp.entity.TwmpLogLogin" >
    <!--
      WARNING - @mbg.generated
    -->
    <id column="login_id" property="loginId" jdbcType="BIGINT" />
    <result column="token" property="token" jdbcType="VARCHAR" />
    <result column="user_name" property="userName" jdbcType="VARCHAR" />
    <result column="user_id" property="userId" jdbcType="BIGINT" />
    <result column="role_id" property="roleId" jdbcType="BIGINT" />
    <result column="role_name" property="roleName" jdbcType="VARCHAR" />
    <result column="login_time" property="loginTime" jdbcType="TIMESTAMP" />
    <result column="logout_time" property="logoutTime" jdbcType="TIMESTAMP" />
    <result column="dpartment_id" property="dpartmentId" jdbcType="BIGINT" />
    <result column="department_name" property="departmentName" jdbcType="VARCHAR" />
  </resultMap>

  <resultMap id="LoginLogResultMap" type="com.ceiec.twmp.tmp.vo.systemlog.login.result.LoginLogResultVO" >
    <id column="login_id" property="operateId" jdbcType="BIGINT" />
    <result column="user_name" property="userName" jdbcType="VARCHAR" />
    <result column="role_name" property="roleName" jdbcType="VARCHAR" />
    <result column="login_time" property="loginTimeDate" jdbcType="TIMESTAMP" />
    <result column="logout_time" property="logoutTimeDate" jdbcType="TIMESTAMP" />
    <result column="department_name" property="departmentName" jdbcType="VARCHAR" />
  </resultMap>


  <select id="queryLoginLogByPage" parameterType="com.ceiec.twmp.tmp.vo.systemlog.login.query.LoginLogQueryVO" resultMap="LoginLogResultMap" >
    select log.login_id login_id, log.user_name user_name, log.login_time login_time, log.logout_time logout_time,
    log.role_name role_name, log.department_name department_name
    from twmp_log_login log
    where 1 = 1
    <if test="userName != null and userName != ''" >
      and log.user_name like concat('%', #{userName}, '%')
    </if>
    <if test="startTime != null" >
      and unix_timestamp(log.login_time) &gt;=  unix_timestamp(#{startTime})
    </if>
    <if test=" endTime != null" >
      and (unix_timestamp(log.login_time) &lt;= unix_timestamp(#{endTime}))
    </if>
    and log.department_id in
    <foreach collection="departmentIds" item="id" index="index" open="(" close=")" separator="," >
      #{id}
    </foreach>
  </select>

  <insert id="insertLogList" parameterType="java.util.List" >
    insert into twmp_log_operate (operate_id, operate_name, operate_type, operate_object_type, operate_time, operator,
      operator_id, department_id, department_name)
    values
    <foreach collection="list" item="log" index="index" separator="," >
      ( #{log.operateId}, #{log.operateName}, #{log.operateType} , #{log.operateObjectType} , #{log.operateTime},
      #{log.operator}, #{log.operatorId} , #{log.departmentId} , #{log.departmentName})
    </foreach>
  </insert>

  <select id="getLoginLogList" parameterType="com.ceiec.twmp.tmp.vo.systemlog.login.query.LoginLogQueryVO" resultMap="LoginLogResultMap" >
    select login_id, user_name, login_time, logout_time, role_name, department_name
    from twmp_log_login
    where 1 = 1
    <if test="userName != null and userName != ''" >
      and user_name like concat('%', #{userName}, '%')
    </if>
    <if test="startTime != null" >
      and unix_timestamp(login_time) &gt;=  unix_timestamp(#{startTime})
    </if>
    <if test=" endTime != null" >
      and (unix_timestamp(login_time) &lt;= unix_timestamp(#{endTime}))
    </if>
    and department_id in
    <foreach collection="departmentIds" item="id" index="index" open="(" close=")" separator="," >
      #{id}
    </foreach>
    order by login_time, logout_time desc
  </select>
</mapper>


