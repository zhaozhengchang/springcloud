<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ceiec.twmp.tmp.mapper.TwmpGisAddressMapper" >
  <resultMap id="BaseResultMap" type="com.ceiec.twmp.tmp.entity.TwmpGisAddress" >
    <!--
      WARNING - @mbg.generated
    -->
    <id column="address_id" property="addressId" jdbcType="BIGINT" />
    <result column="address_name" property="addressName" jdbcType="VARCHAR" />
    <result column="point" property="point" jdbcType="VARCHAR" />
    <result column="deleted" property="deleted" jdbcType="TINYINT" />
  </resultMap>



  <select id="queryAddressByName" resultMap="BaseResultMap">
    select * from twmp_gis_address where deleted = 1 and address_name like concat('%', #{addressName}, '%') order by length (address_name) limit #{num}
  </select>
</mapper>